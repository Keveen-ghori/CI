<script>
    $('input[type="radio"][name="rating"]').click(function () {
       
        var rating = $(this).data('rating');
        var missionId = '@Model.MissionId';
        $.ajax({
            url: '/Home/RateMission',
            type: 'POST',
            data: { missionId: missionId, rating: rating },
            success: function (result) {
                if (result.success) {
                    // Update the UI to reflect the new rating
                    $('input[type="radio"][name="rating"]').prop('checked', false);
                    $('input[type="radio"][name="rating"][data-rating="' + rating + '"]').prop('checked', true);
                } else {
                    console.log(result.message);
                }
            },
            error: function (xhr, status, error) {
                console.log(xhr.responseText);
            }
        });
    });

</script>