                                @{
                                    if (Model.Rating == null)
                                    {
                                        <input type="radio" name="rating" value="5" id="5" data-rating="5">

                                        <label for="5">☆</label>
                                        <input type="radio" name="rating" value="4" id="4" data-rating="4">

                                        <label for="4">☆</label>
                                        <input type="radio" name="rating" value="3" id="3" data-rating="3">

                                        <label for="3">☆</label>
                                        <input type="radio" name="rating" value="2" id="2" data-rating="2">

                                        <label for="2">☆</label>
                                        <input type="radio" name="rating" value="1" id="1" data-rating="1">

                                        <label for="1">☆</label>
                                    }
                                    else
                                    {
                                        for (int i = 1; i <= 5; i++)
                                        {
                                            <input type="radio" name="rating" value="@i" id="@i" @(i == Model.Rating ? "checked" : "")>
                                            <label for="@i">@((i <= Model.Rating) ? "☆" : "☆")</label>
                                        }
                                    }
                                }

                            </span>
                        </h1>
