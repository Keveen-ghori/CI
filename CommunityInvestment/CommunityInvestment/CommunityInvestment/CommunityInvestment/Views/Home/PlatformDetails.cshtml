}



@model CommunityInvestment.Models.spModels.MissionDetails

@{
    bool isUserApplied = Model.is_applied;

    bool disableButton = !(isUserApplied);

    string goalText = Model.goal_objective_text;
    int achievedValue = 0;

    Match match = Regex.Match(goalText, @"\d+");
    if (match.Success)
    {
        achievedValue = int.Parse(match.Value);
    }




}

